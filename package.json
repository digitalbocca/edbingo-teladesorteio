{
  "name": "edbingo-teladesorteio",
  "version": "1.13.0",
  "config": {
    "commitMessage": "Atualizado CDN Bootstrap"
  },
  "parcelCleanPaths": [
    "docs"
  ],
  "private": true,
  "scripts": {
    "dev": "parcel ./src/index.pug -p 8080 -d ./docs --no-cache",
    "build": "parcel build ./src/index.pug -d ./docs --no-cache --no-source-maps --public-url ./",
    "prod": "http-server ./docs -o -c-1",
    "git": "npm run gst && npm run gad && npm run gco && npm run gta && npm run gpu",
    "gst": "git status",
    "gad": "git add .",
    "gco": "cross-var git commit -m \"$npm_package_config_commitMessage\"",
    "gta": "cross-var git tag v$npm_package_version",
    "gpu": "npm run gpo && npm run gpb && npm run gpg",
    "gpo": "git push origin master --tags",
    "gpg": "git push gitlab master --tags",
    "gpb": "git push bitbucket master --tags"
  },
  "dependencies": {
    "animate.css": "^3.7.2",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.1",
    "cross-var-no-babel": "^1.2.0",
    "http-server": "^0.12.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "pug": "^2.0.4",
    "sass": "^1.25.0",
    "standard": "^14.3.1",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
